/* pages/register/register.wxss */

#login {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.login_phcolor {
  font-size: 26rpx;
  color: #dfdfdf;
}

#login input {
  width: 300rpx;
  height: 77rpx;
  border-bottom: 1rpx solid #dfdfdf;
  padding-left: 100rpx;
  padding-right: 100rpx;
  display:inline-block;
  vertical-align:bottom;

}

.login_btn {
  display: inline-block;
  width: 553rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #f63e5a;
  font-size: 30rpx;
  color: #fff;
  margin-bottom: 22rpx;
  border-radius: 100rpx;
  text-align: center;
}
.loginBtnBox{
  margin:50rpx 0 0 0;
}